{
  "version": "0.2.0",
  "configurations": [
  {
    "address": "https://crawler-node-webapp.herokuapp.com",
    "localRoot": "${workspaceFolder}",
    "name": "Attach to Remote",
    "port": 9090,
    "remoteRoot": "/actors",
    "request": "attach",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "pwa-node"
  },
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/page-collector/server.js",
      "request": "launch",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },

    {
      "name": "Launch gpg",
      "program": "${workspaceFolder}/page-collector/generateGbp.js",
      "request": "launch",
      "envFile": "${workspaceFolder}/.env",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    }
 
  ]
}