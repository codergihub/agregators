{
  "name": "actor-quick-start",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/github": "4.0.0",
    "@cucumber/cucumber": "7.0.0",
    "@rollup/plugin-replace": "2.4.1",
    "console-table-printer": "2.9.0",
    "directory-tree": "2.2.9",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "firebase-admin": "9.9.0",
    "fs-extra": "9.1.0",
    "gpg": "^0.6.0",
    "jsdom": "16.5.1",
    "make-dir": "3.1.0",
    "mocha": "8.3.0",
    "node-fetch": "2.6.1",
    "request": "2.88.2",
    "rollup-plugin-cpy": "2.0.1",
    "sinon": "9.2.4"
  },
  "devDependencies": {
    "@open-wc/rollup-plugin-html": "1.2.5",
    "axios": "0.21.1",
    "cross-env": "7.0.2",
    "eslint": "7.21.0",
    "jimp": "0.16.1",
    "node-watch": "0.7.1",
    "nodemon": "2.0.7",
    "puppeteer": "7.1.0",
    "rollup": "2.39.0",
    "rollup-plugin-clear": "2.0.7",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-copy-assets": "2.0.3",
    "rollup-plugin-css-porter": "1.0.2",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-serve": "1.1.0"
  },
  "scripts": {
    "bookCollector": "projectName=books NEXT_TASK=page_collection ALL=TRUE node page-collector/server.js",
    "taskSequelizer": "projectName=defacto NEXT_TASK=page_collection ALL=TRUE node page-collector/server.js",
    "dataCollector": "projectName=defacto NEXT_TASK=page_data_collection node page-collector/server.js --inspect-brk",
    "imageCollector": "projectName=defacto NEXT_TASK=page_image_collection node page-collector/server.js",
    "imageCropper": "projectName=defacto NEXT_TASK=page_image_crop node  page-collector/server.js",
    "imageBlur": "projectName=defacto NEXT_TASK=page_image_blur node  page-collector/server.js",
    "imageEmbed": "projectName=defacto NEXT_TASK=page_image_embed node  page-collector/server.js",
    "pageNavDataTreeCreation": "projectName=defacto NEXT_TASK=page_nav_data_tree_creation node page-collector/server.js",
    "pageLeavesCreation": "projectName=defacto NEXT_TASK=page_leaves_creation node page-collector/server.js",
    "pageGenerator": "projectName=defacto NEXT_TASK=page_generation ALL=FALSE node page-collector/server.js",
    "pageComponentAttachment": "projectName=defacto NEXT_TASK=page_component_attacher ALL=FALSE node page-collector/server.js",
    "pageScriptAttachment": "projectName=defacto NEXT_TASK=page_script_attacher ALL=FALSE node page-collector/server.js",
    "pageNavItems": "projectName=defacto NEXT_TASK=page_nav_items ALL=FALSE  node page-collector/server.js",
    "pageBuilder": "projectName=defacto NEXT_TASK=page_builder ALL=FALSE node page-collector/server.js",
    "metaCreator": "node page-meta-creator/server.js",
    "pagePrerender": "node page-prerender/server.js",
    "dev": "NODE_ENV=dev npm run pageBuilder & NODE_ENV=dev  npm run metaCreator & NODE_ENV=dev node page-serve/server.js",
    "serve": "NODE_ENV=dev node page-serve/server.js",
    "storybook": "NODE_ENV=storybook rollup -c -w",
    "test": "mocha test/**/*.test.js",
    "prerender:test": "mocha page-prerender/test",
    "pageCollector:test": "mocha page-collector/test",
    "promiseConcur:test": "mocha page-collector/test/promiseConcurrencyManager.test.js",
    "imageUpload:test": "mocha page-collector/test/imageUpload.test.js",
    "taskSequelizer:test": "mocha page-collector/test/taskSequelizer.test.js",
    "pageDataCorrector:test": "mocha page-data-corrector/test",
    "pageDataCategorizer:test": "mocha page-data-categorizer/test",
    "pageMetaCreator:test": "mocha page-meta-creator/test",
    "pageImageOptimizer:test": "mocha page-image-optimizer/test",
    "pagePrerender:test": "mocha page-prerender/test",
    "gpg":"node page-collector/generateGbp.js --inspect-brk"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "apify_storage/*",
      "page-build/*",
      "page-data/*",
      "page-meta/*",
      "utils/*"
    ],
    "delay": "2500"
  }
}
