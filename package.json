    "prerender:test": "mocha page-prerender/test",
    "pageCollector:test": "mocha page-collector/tr/moda/test",
    "pageDataCorrector:test": "mocha page-data-corrector/test",
    "pageDataCategorizer:test": "mocha page-data-categorizer/test",
    "pageMetaCreator:test": "mocha page-meta-creator/test",
    "pageImageOptimizer:test": "mocha page-image-optimizer/test",
    "pagePrerender:test": "mocha page-prerender/test"
