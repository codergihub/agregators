    "puppeteer": "7.1.0",
    "rollup": "2.39.0",
    "rollup-plugin-clear": "2.0.7",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-copy-assets": "2.0.3",
    "rollup-plugin-css-porter": "1.0.2",
    "rollup-plugin-delete": "2.0.0",
    "rollup-plugin-serve": "1.1.0"
  },
  "scripts": {
    "dataCollector": "NEXT_TASK=page_collection ALL=FALSE node page-collector/server.js",
    "mergeFiles": "NEXT_TASK=page_merge_files ALL=FALSE node page-collector/server.js",
    "exportExcel": "NEXT_TASK=page_export_excel ALL=FALSE node page-collector/server.js",
    "uploadExcel": "NEXT_TASK=page_upload_excel ALL=FALSE node page-collector/server.js",
    "imageCollector": "NEXT_TASK=page_image_collection ALL=FALSE  node page-collector/server.js",
    "imageUpload": "NEXT_TASK=page_upload_image ALL=FALSE  node page-collector/server.js",
    "imageCropper": "NEXT_TASK=page_image_crop ALL=FALSE node  page-collector/server.js",
    "imageBlur": "NEXT_TASK=page_image_blur ALL=FALSE node  page-collector/server.js",
    "imageEmbed": "NEXT_TASK=page_image_embed ALL=FALSE node  page-collector/server.js",
    "pageNavDataTreeCreation": "NEXT_TASK=page_nav_data_tree_creation ALL=FALSE node page-collector/server.js",
    "pageLeavesBy100": "NEXT_TASK=page_leaves_creation ALL=FALSE node page-collector/server.js",
    "pageGenerator": "projectName=moda NEXT_TASK=page_generation ALL=FALSE node page-collector/server.js",
    "pageNavItems": "projectName=moda NEXT_TASK=page_nav_items ALL=FALSE  node page-collector/server.js",
    "pageBuilder": "projectName=moda NEXT_TASK=page_builder ALL=FALSE node page-collector/server.js",
    "pagePrerender": "projectName=moda NEXT_TASK=page_prerender ALL=FALSE  node page-collector/server.js",
    "taskSequelizer": "ALL=TRUE node page-collector/server.js",
    "serve": "NODE_ENV=dev projectName=moda node page-serve/server.js",
    "storybook": "NODE_ENV=storybook rollup -c -w",
    "test": "mocha test/**/*.test.js",
    "prerender:test": "mocha page-prerender/test",
    "pageCollector:test": "NODE_ENV=test mocha page-collector/test/**/*.test.js",
    "test:utils:promiseConcurrency": "mocha utils/promise-concurrency/test",
    "promiseConcur:test": "mocha page-collector/test/promiseConcurrencyManager.test.js",
    "imageUpload:test": "mocha page-collector/test/imageUpload.test.js",
    "taskSequelizer:test": "mocha page-collector/test/taskSequelizer.test.js",
    "pageDataCorrector:test": "mocha page-data-corrector/test",
    "pageDataCategorizer:test": "mocha page-data-categorizer/test",
    "pageMetaCreator:test": "mocha page-meta-creator/test",
    "pageImageOptimizer:test": "mocha page-image-optimizer/test",
    "pagePrerender:test": "mocha page-prerender/test",
    "snipe": "node snipe.js",
    "start": "node index.js"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "apify_storage/*",
      "page-build/*",
      "page-data/*",
      "page-meta/*",
      "utils/*"
    ],
    "delay": "2500"
  }
}
