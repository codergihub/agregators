    "express": "4.17.1",
    "firebase-admin": "9.9.0",
    "form-data": "^4.0.0",
    "fs-extra": "9.1.0",
    "gpg": "^0.6.0",
    "jsdom": "16.5.1",
    "make-dir": "3.1.0",
    "mocha": "8.3.0",
    "node-blob": "0.0.2",
    "node-fetch": "2.6.1",
    "request": "2.88.2",
    "rollup-plugin-cpy": "2.0.1",
    "sinon": "9.2.4"
  },
  "devDependencies": {
    "@open-wc/rollup-plugin-html": "1.2.5",
    "axios": "0.21.1",
    "cross-env": "7.0.2",
    "eslint": "7.21.0",
    "jimp": "0.16.1",
