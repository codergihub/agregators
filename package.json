  "scripts": {
    "pageCollector": "NODE_ENV=page_collection node page-collector/server.js",
    "dataCollector": "NODE_ENV=page_data_collection node page-collector/server.js",
    "imageCollector": "NODE_ENV=page_image_collection node page-collector/server.js",
