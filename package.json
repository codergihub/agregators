{
  "name": "actor-quick-start",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@cucumber/cucumber": "^7.0.0",
    "apify": "^1.0.1",
    "dotenv": "^8.2.0",
    "mocha": "^8.3.0",
    "sinon": "^9.2.4"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.7",
    "puppeteer": "^7.1.0",
    "rollup": "^2.39.0",
    "rollup-plugin-serve": "^1.1.0"
  },
  "scripts": {
